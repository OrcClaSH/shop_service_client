      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "noEmit": true,
      "allowJs": true,
      "sourceMap": true,
      "strict": true,
      "strictNullChecks": true,
      "skipLibCheck": true,
      "target": "es5",
      "module": "esnext",
      "isolatedModules": true,
      "moduleResolution": "node",
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true
  },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "node_modules"
  ]
